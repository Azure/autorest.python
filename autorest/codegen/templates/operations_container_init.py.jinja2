{% set async_suffix = "_async" if async_mode else "" %}
{# actual template starts here #}
# coding=utf-8
{{ code_model.options['license_header'] }}

{% for operation_group in operation_groups %}
  {% if operation_group.is_empty_operation_group %}
from ._{{ code_model.module_name }}_operations{{ async_suffix }} import {{ operation_group.class_name }}
  {% else %}
from ._{{ operation_group.name }}_operations{{ async_suffix }} import {{ operation_group.class_name }}
  {% endif %}
{% endfor %}

__all__ = [
{% for operation_group in operation_groups %}
    '{{ operation_group.class_name }}',
{% endfor %}
]
