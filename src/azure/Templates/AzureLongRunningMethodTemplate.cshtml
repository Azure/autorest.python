        request = self._client.@(MethodPy.GetHttpFunction(Model.HttpMethod))(url, query_parameters)
@{string sendCall = Model.GetSendCall(AsyncMode);}
@if (Model.RequestBody != null)
{
        @:response = @(sendCall)(
