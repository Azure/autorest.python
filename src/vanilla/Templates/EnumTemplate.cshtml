@using System.Linq
@using AutoRest.Python
@inherits AutoRest.Core.Template<System.Collections.Generic.IEnumerable<AutoRest.Core.Model.EnumType>>
# coding=utf-8
# --------------------------------------------------------------------------
@Header("# ").TrimMultilineHeader()
# --------------------------------------------------------------------------
@EmptyLine
from enum import Enum
@{
  foreach (var enumType in @Model)
  {
@EmptyLine
@EmptyLine
@:class @(enumType.Name)(Enum):
@EmptyLine
    for (int i = 0; i < enumType.Values.Count; i++)
    {
@:    @(enumType.Values[i].MemberName) = "@enumType.Values[i].SerializedName"
    }
  }
}
